{
  "name": "typescript-openapi-router",
  "version": "0.1.29",
  "description": "Multi purpose yet simple nodejs router with OpenAPI 3 compatibility for REST APIs",
  "author": {
    "name": "Vali Draganescu",
    "email": "vali.draganescu88+github@gmail.com"
  },
  "license": "https://opensource.org/licenses/EUPL-1.2",
  "keywords": [
    "nodejs",
    "typescript",
    "serverless",
    "router"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:ValiDraganescu/openapi-router.git"
  },
  "scripts": {
    "build": "./node_modules/typescript/bin/tsc",
    "test": "jest",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "postversion": "git push && git push --tags",
    "version": "yarn format && git add -A src",
    "format": "./node_modules/prettier/bin-prettier.js --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.46",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.5.0",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ts-jest": "^25.0.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist/**/*"
  ]
}
